function [ error_rates ] = run_cv_for_error( input_args )
% Performs 10-fold cross validation to get the error rates per fold per
% emotion


error_stats = struct('error_rates',[],'confusion_matrix',[],'error_rate',[],'precision_rates',[], 'recall_rates',[],'F1_measures',[]);
for i = 1:num_folds
        [~, error_rates] = cross_validate(randomised_examples,randomised_targets, i);
end

end

